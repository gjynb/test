<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.dao.UserDao">
    <!--新增用户-->
    <insert id="saveUser" parameterType="com.example.demo.entity.UserInfo">
        insert into t_user
            (   id,
                name,
                sex,
                isDelet,
                passwd,
                acount,
                age
            )
        values
           (    #{id},
                #{name},
                #{sex},
                0,
                #{passwd},
                #{acount},
                #{age}
            )
    </insert>
</mapper>